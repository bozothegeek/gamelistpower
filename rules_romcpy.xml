<?xml version="1.0" encoding="utf-8"?>
<!--NORDIC POWER-->
<rules>
	<options>
		<!-- AUTORISE %%NAME% %%name%% %%PLATEFORM%% %region%% %REGION%% -->
		<titleformat>%%NAME%% (%%PLATEFORM%%)</titleformat>
		<path>/recalbox/share/roms/collections2</path>
	</options>
  <rule>
    <name>addams1</name>
    <searchAttribute>name</searchAttribute>
    <searchValue>addams</searchValue>
    <destination>/recalbox/share/roms/collections2/addams</destination>
    <preserveFavorite>false</preserveFavorite>
  </rule>
  <rule>
    <name>zombies1</name>
    <searchAttribute>name</searchAttribute>
    <searchValue>zombies</searchValue>
    <destination>/recalbox/share/roms/collections2/zombies</destination>
    <preserveFavorite>false</preserveFavorite>
  </rule>
  <rule>
    <name>mario</name>
    <searchAttribute>name</searchAttribute>
    <searchValue>mario</searchValue>
    <destination>/recalbox/share/roms/collections2/mario</destination>
    <exclusions>
    	<exclusion>Mario Lemieux Hockey.zip</exclusion>
    	<exclusion>/recalbox/share/roms/mario</exclusion>
    </exclusions>
    <preserveFavorite>false</preserveFavorite>
  </rule>
</rules>